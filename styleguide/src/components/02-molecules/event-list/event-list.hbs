<ul class="event-list
    {{~#if customClasses}}
        {{~#each customClasses}} {{this}}{{/each~}}
    {{/if~}}
    "
    {{~#if customAttributes}}
        {{~#each customAttributes}}
            {{{this.attribute}}}{{#if this.value}}="{{{this.value}}}"{{/if}}
        {{/each~}}
    {{/if~}}
>
    {{#each events}}
        <li class="event-list__item">
            {{render '@event-fragments' this merge=true~}}
        </li>
    {{/each}}
</ul>
